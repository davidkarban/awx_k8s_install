---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: awx
  namespace: {{ namespace }}
spec:
  ingressClassName: nginx
  rules:
  - http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: awx-service
            port:
              number: 80
